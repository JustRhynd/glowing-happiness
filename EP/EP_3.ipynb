    "#Exercice 1\n",
    "def taille(arb):\n",
    "    if len(arb) == 0:\n",
    "        return 0\n",
    "\n",
    "    for i in range(1, len(arb)):\n",
    "        return 1 + taille(arb[i])\n",
    "    "
